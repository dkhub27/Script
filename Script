local bannedUsersUrl = "https://gist.githubusercontent.com/dkhub27/ba317f005893097a9609c470bc0e9397/raw/example.txt"
local scriptExecuted = false

-- Função para verificar se o jogador está banido
local function checkIfPlayerBanned()
    local bannedList = game:HttpGet(bannedUsersUrl)
    local playerName = game.Players.LocalPlayer.Name

    -- Converte o conteúdo para uma tabela de strings
    local bannedUsers = {}
    for line in bannedList:gmatch("[^\r\n]+") do
        local bannedUser = line:match("^(.-) %-")
        if bannedUser then
            table.insert(bannedUsers, bannedUser)
        end
    end

    -- Verifica se o nome do jogador está na lista de banidos
    for _, bannedUser in ipairs(bannedUsers) do
        if playerName == bannedUser then
            toclipboard("https://discord.gg/dkhub") -- Copia o link do Discord para o clipboard
            game.Players.LocalPlayer:Kick("You are banned from using this script. To get unbanned, contact us on Discord (link copied) and pay the unban fee of 2€.")
            return true
        end
    end

    return false
end

-- Verifica se o jogador está banido antes de executar o script
if not checkIfPlayerBanned() then
    -- Executa o script se o jogador não estiver banido
    local function executeScript()
        if not scriptExecuted then
            local scriptUrl = scripts[game.PlaceId]

            if scriptUrl then
                loadstring(game:HttpGet(scriptUrl))()
                scriptExecuted = true
            else
                toclipboard("https://discord.gg/dkhub")
                game.Players.LocalPlayer:Kick("Game Not Supported, Discord link copied to clipboard.")
            end
        end
    end

    -- Executa o script pela primeira vez
    executeScript()

    -- Verifica a cada 60 segundos se o jogador foi banido
    while true do
        wait(60)
        checkIfPlayerBanned()
    end
end
